    "extra": {
        "merge-plugin": {
            "include": [
                "modules/catalog/*/composer.module.json",
                "modules/localplay/*/composer.module.json",
                "modules/plugins/*/composer.module.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        }
    },
   "scripts": {
        "post-autoload-dump": [
            "@php scripts/composer/check_tag_tools.php"
        ]
     },
   "autoload": {
        "psr-4": {
            "Lib\\": "lib/class",
            "Beets\\": "modules/Beets"
        }
    }
}